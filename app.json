{
  "expo": {
    "name": "flickpad",
    "slug": "flickpad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-flickpad-removebg-preview-2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-flickpad-removebg-preview.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.itaj.flickpad"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon-flickpad-removebg-preview.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo-flickpad-removebg-preview.png",
          "imageWidth": 80,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "76350738-ce8f-4b9c-8f16-1e8989bc010f"
      }
    }
  }
}